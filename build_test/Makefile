# Build rule for target.
extinst_tables: cmake_check_build_system
	$(MAKE) $(MAKESILENT) -f CMakeFiles/Makefile2 extinst_tables
.PHONY : extinst_tables

# fast build rule for target.
extinst_tables/fast:
	$(MAKE) $(MAKESILENT) -f RHI/Vulkan/External/SPIRV-Tools/source/CMakeFiles/extinst_tables.dir/build.make RHI/Vulkan/External/SPIRV-Tools/source/CMakeFiles/extinst_tables.dir/build
.PHONY : extinst_tables/fast

